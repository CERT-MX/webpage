body
  background: gray
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

#bubble-able
  float: left
  width: 80px
  height: 250px
  background: none repeat scroll 0% 0% #4682B4
  position: absolute
  left: 0
  border-top-right-radius: 30px
  border-bottom-right-radius: 30px
  border-bottom: 1px solid white
  border-right: 1px solid white

#bubble-able > div
  float: left
  width: 56px
  height: 56px
  background: #f5f5f5
  margin:10px
  border-radius: 100%
  margin-left: 0.02in

#bubble-able > div:hover
  background: red
